{
  "name": "gatsby-apollo-typescript-starter_api",
  "version": "0.0.1",
  "description": "Apollo / GraphQL API",
  "engines": {
    "node": ">= 12.13 <13"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "node build/app",
    "dev": "concurrently \"npm run build:watch\" \"nodemon build/app\"",
    "doc": "typedoc --out ./docs/ ./src",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint --fix . --ext .ts,.tsx",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/graphql-resolvers": "^0.2.2",
    "@types/jest": "~25.1.2",
    "@types/mongoose": "^5.7.14",
    "@types/node": "~12.12.27",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "~2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "axios": "^0.19.2",
    "concurrently": "^5.2.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-jest": "~23.7.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "~25.1.0",
    "jest-worker": "^25.4.0",
    "lint-staged": "^10.2.1",
    "nodemon": "^2.0.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "~25.2.0",
    "tsutils": "~3.17.0",
    "typedoc": "^0.17.4",
    "typescript": "~3.8.2"
  },
  "author": "DL Ford <hello@dlford.io>",
  "license": "UNLICENCED",
  "dependencies": {
    "apollo-server": "^2.12.0",
    "apollo-server-express": "^2.12.0",
    "bcrypt": "^4.0.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-resolvers": "^0.4.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
    "validator": "^13.0.0"
  }
}
