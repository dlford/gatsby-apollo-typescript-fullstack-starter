{
  "name": "gatsby-typescript-starter-kitchen-sink",
  "description": "Fullstack Gatsby Apollo TypeScript starter - Front End",
  "version": "0.0.1",
  "author": "DL Ford <hello@dlford.io>",
  "scripts": {
    "precommit": "lint-staged",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore --fix . --ext ts --ext tsx --ext js --ext jsx",
    "deploy:github": "rimraf node_modules/gh-pages/.cache && gatsby build --prefix-paths && gh-pages -d public",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.*(json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@openfonts/open-sans_latin-ext": "^1.44.1",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.3.9",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-upload-client": "^13.0.0",
    "apollo-utilities": "^1.3.3",
    "babel-plugin-tailwind-components": "^0.5.10",
    "cors": "^2.8.5",
    "gatsby": "^2.29.1",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-create-client-paths": "^2.3.0",
    "gatsby-plugin-emotion": "^4.2.3",
    "gatsby-plugin-manifest": "^2.3.5",
    "gatsby-plugin-offline": "^3.1.4",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-sharp": "^2.11.1",
    "gatsby-plugin-typescript": "^2.3.3",
    "gatsby-source-filesystem": "^2.2.4",
    "gatsby-transformer-sharp": "^2.5.1",
    "gatsby-transformer-typescript-css-modules": "^1.0.14",
    "graphql-tag": "^2.10.3",
    "jsonwebtoken": "^8.5.1",
    "react": "^16.12.0",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "rimraf": "^3.0.2",
    "subscriptions-transport-ws": "^0.9.16",
    "tailwindcss": "^1.3.4",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "twin.macro": "^1.0.0-alpha.10",
    "typeface-montserrat": "0.0.75",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.3",
    "@types/apollo-upload-client": "^8.1.3",
    "@types/cors": "^2.8.6",
    "@types/jest": "^25.2.1",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.34",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-jest": "^25.4.0",
    "babel-plugin-remove-graphql-queries": "^2.8.5",
    "babel-preset-gatsby": "^0.3.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.2",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.0.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.3.4",
    "twin.macro": "^1.0.0-alpha.10",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "apollo",
    "fullstack"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dlford/gatsby-apollo-typescript-starter.git"
  },
  "bugs": {
    "url": "https://github.com/dlford/gatsby-apollo-typescript-starter/issues"
  }
}
